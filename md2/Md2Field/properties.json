{
  "md2": {
    "field": {
      "@style": "{d.field}",
      "mixin|m": {
        "mixin|appearance": {
          "mixin|filled": {
            "var|sys": {
              "backgroundOpacity": "{sys.opacity.overlay}",
              "borderOpacity": "{sys.opacity.primary.disabled}",
              "hover": {
                "borderOpacity": "{sys.opacity.primary.medium}"
              },
              "focus": {
                "border": "{??sys.palette}",
                "borderOpacity": "{sys.opacity.primary.high}"
              }
            }
          }
        }
      },
      "var|sys": {
        "palette": "{sys.palette.primary}",
        "background": "{sys.palette.overlay}",
        "backgroundOpacity": "0",
        "border": "{sys.palette.onSurface}",
        "borderOpacity": "{sys.opacity.border}",
        "borderWidth": "{ref.spacing.px}",
        "gradient": "{sys.palette.onSurface}",
        "gradientOpacity": "0",
        "height": "{ref.spacing.14}",
        "padding": "{ref.spacing.3}",
        "shadow": "none",
        "space": "{ref.spacing.1}",
        "text": "{sys.palette.onSurface}",
        "textOpacity": "{sys.opacity.surface.high}",
        "width": "{ref.spacing.72}",
        "label": {
          "height": "{ref.spacing.6}",
          "required": "{sys.palette.error}",
          "textOpacity": "{sys.opacity.surface.medium}"
        },
        "prefix": {
          "textOpacity": "{sys.opacity.surface.medium}"
        },
        "disabled": {
          "textOpacity": "{sys.opacity.surface.disabled}"
        },
        "hover": {
          "gradientOpacity": "{sys.opacity.hover}"
        },
        "focus": {
          "borderWidth": "{ref.spacing.px} * 2",
          "gradientOpacity": "{sys.opacity.focus}",
          "label": {
            "text": "{??sys.palette}",
            "textOpacity": "{sys.opacity.surface.high}"
          }
        }
      },
      "@font": "{font.subtitle1}",
      "border-radius": "{sys.rounded.standard}",
      "height": {
        "xm": "{ref.spacing.10}",
        "sm": "{ref.spacing.12}",
        "md": "{ref.spacing.14}",
        "lg": "{ref.spacing.16}",
        "xl": "{ref.spacing.20}"
      },
      "section|appearance": {
        "filled": {
          "@style": "{??m.appearance.filled}"
        },
        "default": "filled"
      },
      "section|rounded": {
        "@style": "{?rounded}"
      }
    }
  }
}
