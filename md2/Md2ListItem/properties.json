{
  "md2": {
    "list-item": {
      "@style": "{d.list-item}",
      "var|sys": {
        "palette": "{sys.palette.primary}",
        "paddingX": "{ref.spacing.4}",
        "paddingY": "{ref.spacing.1}",
        "height": "{ref.spacing.12}",
        "text": "{sys.palette.onSurface}",
        "textOpacity": "{sys.opacity.surface.high}",
        "border": "{sys.palette.border}",
        "borderOpacity": "{sys.opacity.border}",
        "borderWidth": "{ref.spacing.px}",
        "icon": {
          "margin": "{ref.spacing.6}"
        },
        "thumbnail": {
          "marginLeft": "{ref.spacing.2} * -1",
          "marginRight": "{ref.spacing.4}",
          "size": "{ref.spacing.8}"
        },
        "trailing": {
          "margin": "{ref.spacing.4}"
        },
        "checkbox": {
          "compensation": "{ref.spacing.6}"
        },
        "prefix": {
          "padding": "{ref.spacing.4}"
        },
        "suffix": {
          "padding": "{ref.spacing.6}"
        },
        "description": {
          "textOpacity": "{sys.opacity.surface.medium}"
        },
        "overlay": {
          "backgroundOpacity": "{sys.opacity.selected}"
        },
        "highlight": {
          "palette": "{sys.palette.warning}",
          "backgroundOpacity": "{sys.opacity.selected}"
        },
        "selected": {
          "palette": "{sys.palette.primaryVariant}",
          "backgroundOpacity": "{sys.opacity.selected}"
        },
        "progress": {
          "size": "{ref.spacing.8}",
          "opacity": "{sys.opacity.surface.disabled}"
        },
        "disabled": {
          "palette": "{sys.palette.disabled}",
          "textOpacity": "{sys.opacity.surface.disabled}"
        },
        "hover": {
          "gradientOpacity": "{sys.opacity.hover}"
        },
        "focus": {
          "gradientOpacity": "{sys.opacity.focus}"
        },
        "active": {
          "gradientOpacity": "{sys.opacity.dragged}"
        },
        "rounded": {
          "marginX": "{ref.spacing.2}",
          "marginY": "{ref.spacing.1}"
        },
        "dense": {
          "paddingX": "{ref.spacing.2}",
          "icon": {
            "margin": "{ref.spacing.3}"
          }
        }
      },
      "mixin|m": {
        "mixin|appearance": {
          "contained": {
            "var|sys": {
              "selected": {
                "text": "{sys.palette.onPrimary}",
                "backgroundOpacity": "{sys.opacity.primary.high}"
              }
            }
          }
        }
      },
      "@font": "{font.subtitle1}",
      "@@palette": "{?palette}",
      "height": {
        "xs": {
          "var|sys": {
            "height": "{ref.spacing.10}",
            "thumbnail": {
              "marginLeft": "0px",
              "marginRight": "{ref.spacing.6}"
            },
            "dense": {
              "paddingX": "{ref.spacing.3}"
            }
          }
        },
        "sm": {
          "var|sys": {
            "height": "{ref.spacing.12}",
            "thumbnail": {
              "marginLeft": "{ref.spacing.1} * -1",
              "marginRight": "{ref.spacing.5}"
            },
            "dense": {
              "paddingX": "{ref.spacing.3}"
            }
          }
        },
        "md": {
          "var|sys": {
            "height": "{ref.spacing.14}",
            "dense": {
              "paddingX": "{ref.spacing.3}"
            }
          }
        },
        "lg": {
          "var|sys": {
            "height": "{ref.spacing.18}",
            "dense": {
              "paddingX": "{ref.spacing.3}"
            }
          },
          "& .??__short": {
            "@font": "{font.body1}"
          }
        },
        "xl": {
          "var|sys": {
            "height": "{ref.spacing.18}",
            "thumbnail": {
              "marginLeft": "{??l.paddingX} * -1"
            }
          },
          "& .??__short": {
            "@font": "{font.body1}"
          }
        },
        "default": "sm"
      },
      "#suffix": {
        "@font": "{font.subtitle2}"
      },
      "#description": {
        "@font": "{font.caption}"
      },
      "#short": {
        "@font": "{font.caption}"
      },
      "~appearance": {
        "contained": {
          "@style": "{??m.appearance.contained}"
        },
        "contained-selected": {
          "@style": "{??m.appearance.contained}"
        }
      },
      "~adaptive": {
        "xs": {
          "@style": "{??m.icon}"
        },
        "xs-sm": {
          "media-max|{ref.screen.sm}": {
            "@style": "{??m.icon}"
          }
        },
        "xs-md": {
          "media-max|{ref.screen.md}": {
            "@style": "{??m.icon}"
          }
        },
        "xs-lg": {
          "media-max|{ref.screen.lg}": {
            "@style": "{??m.icon}"
          }
        },
        "sm": {
          "media|{ref.screen.sm}": {
            "@style": "{??m.icon}"
          }
        },
        "sm-md": {
          "media-min-max|{ref.screen.sm} {ref.screen.md}": {
            "@style": "{??m.icon}"
          }
        },
        "sm-lg": {
          "media-min-max|{ref.screen.sm} {ref.screen.lg}": {
            "@style": "{??m.icon}"
          }
        },
        "md": {
          "media|{ref.screen.md}": {
            "@style": "{??m.icon}"
          }
        },
        "md-lg": {
          "media-min-max|{ref.screen.md} {ref.screen.lg}": {
            "@style": "{??m.icon}"
          }
        },
        "lg": {
          "media|{ref.screen.lg}": {
            "@style": "{??m.icon}"
          }
        },
        "full": {}
      },
      "~rounded": {
        "standard": {
          "@style": "{??m.rounded}",
          "var|l": {
            "rounded": "{sys.rounded.standard}"
          }
        },
        "sm": {
          "@style": "{??m.rounded}",
          "var|l": {
            "rounded": "{sys.rounded.sm}"
          }
        },
        "md": {
          "@style": "{??m.rounded}",
          "var|l": {
            "rounded": "{sys.rounded.md}"
          }
        }
      },
      "media|{ref.screen.md}": {
        "var|sys": {
          "paddingX": "{ref.spacing.6}"
        }
      }
    }
  }
}
