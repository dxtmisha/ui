{
  "md2": {
    "list-item": {
      "@style": "{d.list-item}",
      "var|sys": {
        "palette": "{sys.palette.primary}",
        "paddingX": "{ref.spacing.4}",
        "paddingY": "{ref.spacing.1}",
        "height": "{ref.spacing.12}",
        "text": "{sys.palette.onSurface}",
        "textOpacity": "{sys.opacity.surface.high}",
        "gradient": "{sys.palette.onSurface}",
        "icon": {
          "margin": "{ref.spacing.8}"
        },
        "trailing": {
          "margin": "{ref.spacing.4}"
        },
        "prefix": {
          "padding": "{ref.spacing.4}"
        },
        "suffix": {
          "padding": "{ref.spacing.6}"
        },
        "description": {
          "textOpacity": "{sys.opacity.surface.medium}"
        },
        "hover": {
          "gradientOpacity": "{sys.opacity.hover}"
        },
        "focus": {
          "gradientOpacity": "{sys.opacity.focus}"
        },
        "active": {
          "gradientOpacity": "{sys.opacity.dragged}"
        },
        "rounded": {
          "marginX": "{ref.spacing.2}",
          "marginA": "{ref.spacing.2}",
          "marginY": "{ref.spacing.1}"
        },
        "disabled": {
          "palette": "{sys.palette.disabled}",
          "textOpacity": "{sys.opacity.surface.disabled}"
        },
        "highlight": {
          "palette": "{sys.palette.warning}",
          "backgroundOpacity": "{sys.opacity.selected}",
          "text": "{sys.palette.warning}"
        },
        "selected": {
          "palette": "{sys.palette.primaryVariant}",
          "backgroundOpacity": "{sys.opacity.selected}"
        }
      },
      "@font": "{font.subtitle1}",
      "@@palette": "{?palette}",
      "#suffix": {
        "@font": "{font.subtitle2}"
      },
      "#description": {
        "@font": "{font.caption}"
      },
      "section|height": {
        "sm": {
          "var|l": {
            "height": "{ref.spacing.12}"
          }
        },
        "md": {
          "var|l": {
            "height": "{ref.spacing.14}"
          }
        },
        "lg": {
          "var|l": {
            "height": "{ref.spacing.18}"
          }
        }
      },
      "media|{ref.screen.md}": {
        "var|sys": {
          "paddingX": "{ref.spacing.6}"
        }
      }
    }
  }
}
