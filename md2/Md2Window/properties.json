{
  "md2": {
    "window": {
      "@style": "{d.window}",
      "var|sys": {
        "background": "{sys.elevation.background.24}",
        "index": "{sys.elevation.index.24}",
        "padding": "{ref.spacing.10} {ref.spacing.4}",
        "shadow": "{sys.elevation.shadow.24}",
        "scrim": {
          "background": "{sys.palette.onBackground}",
          "opacity": "{sys.opacity.scrim}"
        }
      },
      "section|rounded": {
        "none": {
          "var|rounded": "{sys.rounded.none}"
        },
        "standard": {
          "var|rounded": "{sys.rounded.standard}"
        },
        "sm": {
          "var|rounded": "{sys.rounded.sm}"
        },
        "md": {
          "var|rounded": "{sys.rounded.md}"
        },
        "lg": {
          "var|rounded": "{sys.rounded.lg}"
        },
        "xl": {
          "var|rounded": "{sys.rounded.xl}"
        },
        "default": "standard"
      },
      "section|height": {
        "sm": {
          "var|height": "{ref.spacing.px} * 256"
        },
        "md": {
          "var|height": "{ref.spacing.px} * 512"
        },
        "lg": {
          "var|height": "{ref.spacing.px} * 768"
        },
        "default": "md"
      },
      "section|width": {
        "sm": {
          "var|width": "{ref.spacing.px} * 512"
        },
        "md": {
          "var|width": "{ref.spacing.px} * 768"
        },
        "lg": {
          "var|width": "{ref.spacing.px} * 1024"
        },
        "default": "md"
      }
    }
  }
}
