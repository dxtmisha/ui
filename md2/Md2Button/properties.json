{
  "md2": {
    "button": {
      "@style": "{d.button}",
      "mixin|m": {
        "appearance": {
          "text": {
            "var|??sys": {
              "text": "{??sys.palette}",
              "textOpacity": "{sys.opacity.surface.high}",
              "active": {
                "borderOpacity": "{sys.opacity.surface.medium}"
              },
              "disabled": {
                "text": "{sys.palette.onSurface}",
                "textOpacity": "{sys.opacity.surface.disabled}"
              },
              "selected": {
                "gradient": "{??sys.selected.palette}",
                "text": "{??sys.selected.palette}"
              }
            },
            "palette-color": "{??l.text}",
            "@before": "{??m.before}"
          }
        }
      },
      "var|sys": {
        "palette": "{sys.palette.primary}",
        "borderWidth": "{ref.spacing.px}",
        "dense": "{ref.spacing.2}",
        "padding": "{ref.spacing.4}",
        "space": "{ref.spacing.1}",
        "width": "{ref.spacing.28}",
        "active": {
          "gradientOpacity": "{sys.opacity.dragged}"
        },
        "disabled": {
          "palette": "{sys.palette.disabled}"
        },
        "dragged": {
          "shadow": "{sys.elevation.shadow.8}"
        },
        "focus": {
          "gradientOpacity": "{sys.opacity.focus}"
        },
        "hover": {
          "gradientOpacity": "{sys.opacity.hover}"
        },
        "selected": {
          "palette": "{sys.palette.primaryVariant}"
        },
        "icon": {
          "margin": "{ref.spacing.2}",
          "padding": "{ref.spacing.1}",
          "text": "{ref.font.size.2xl}",
          "textTrailing": "{ref.font.size.xl}"
        }
      },
      "@font": "{font.button}",
      "border-radius": "{sys.rounded.standard}",
      "height": {
        "sm": "{ref.spacing.8}",
        "md": "{ref.spacing.9}",
        "lg": "{ref.spacing.11}",
        "default": "md"
      },
      "width": {
        "auto": {
          "var|??sys": {
            "width": "0"
          }
        },
        "sm": {
          "var|??sys": {
            "width": "{ref.spacing.20}"
          }
        },
        "md": {
          "var|??sys": {
            "width": "{ref.spacing.28}"
          }
        },
        "lg": {
          "var|??sys": {
            "width": "{ref.spacing.36}"
          }
        },
        "full": {
          "var|??sys": {
            "width": "100%"
          }
        }
      },
      "z-index": "{sys.elevation.index.2}",
      "@@palette": "{?palette}",
      "section|adaptive": {
        "sm": {
          "media|{ref.screen.sm}": {
            "@style": "{??m.text}"
          }
        },
        "md": {
          "media|{ref.screen.md}": {
            "@style": "{??m.text}"
          }
        },
        "lg": {
          "media|{ref.screen.lg}": {
            "@style": "{??m.text}"
          }
        },
        "full": {
          "@style": "{??m.text}"
        },
        "icon": {
          "--": "none"
        },
        "default": "full"
      },
      "section|appearance": {
        "contained": {
          "var|??sys": {
            "backgroundOpacity": "{sys.opacity.primary.high}",
            "gradient": "{sys.palette.onPrimary}",
            "shadow": "{sys.elevation.shadow.2}",
            "text": "{sys.palette.onPrimary}",
            "textOpacity": "{sys.opacity.primary.high}",
            "active": {
              "shadow": "{sys.elevation.shadow.8}"
            },
            "disabled": {
              "backgroundOpacity": "{sys.opacity.disabled}",
              "shadow": "none",
              "text": "{sys.palette.onSurface}",
              "textOpacity": "{sys.opacity.surface.disabled}"
            }
          },
          "gradient": "{??l.gradient}"
        },
        "text": {
          "@style": "{??m.appearance.text}",
          "var|??sys": {
            "padding": "{ref.spacing.2}",
            "icon": {
              "padding": "{ref.spacing.1}"
            }
          }
        },
        "outlined": {
          "@style": "{??m.appearance.text}",
          "::after": {
            "@style": "{??m.border}",
            "border-color": "{sys.palette.onSurface}",
            "border-opacity": "{sys.opacity.border}"
          }
        },
        "default": "contained"
      },
      "section|lowercase": {
        "@font": "{font.body1}"
      },
      "section|rounded": {
        "@style": "{?rounded}"
      },
      "&.??--rounded--full": {
        "@style": "{??m.round}"
      }
    }
  }
}
