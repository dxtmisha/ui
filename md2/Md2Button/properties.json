{
  "md2": {
    "button": {
      "@style": "{d.button}",
      "var|sys": {
        "palette": "{sys.palette.primary}",
        "padding": "{ref.spacing.4}",
        "space": "{ref.spacing.1}",
        "height": "{ref.spacing.9}",
        "zIndex": "{sys.elevation.index.2}",
        "text": "{??sys.palette}",
        "textOpacity": "{sys.opacity.surface.high}",
        "borderWidth": "{ref.spacing.px}",
        "rounded": "{sys.rounded.standard}",
        "icon": {
          "padding": "{ref.spacing.1}",
          "margin": "{ref.spacing.2}",
          "fontSize": "{ref.font.size.2xl}"
        },
        "iconTrailing": {
          "fontSize": "{ref.font.size.xl}"
        },
        "dense": {
          "padding": "{ref.spacing.2}",
          "icon": {
            "margin": "{ref.spacing.1}"
          }
        },
        "selected": {
          "palette": "{sys.palette.primaryVariant}"
        },
        "disabled": {
          "palette": "{sys.palette.disabled}"
        },
        "hover": {
          "gradientOpacity": "{sys.opacity.hover}"
        },
        "focus": {
          "gradientOpacity": "{sys.opacity.focus}"
        },
        "active": {
          "gradientOpacity": "{sys.opacity.dragged}",
          "zIndex": "{sys.elevation.index.8}"
        },
        "dragged": {
          "shadow": "{sys.elevation.shadow.8}",
          "zIndex": "{sys.elevation.index.8}"
        }
      },
      "mixin|m": {
        "appearance": {
          "contained": {
            "var|sys": {
              "text": "{sys.palette.onPrimary}",
              "textOpacity": "{sys.opacity.primary.high}",
              "backgroundOpacity": "{sys.opacity.primary.high}",
              "gradient": "{sys.palette.onPrimary}",
              "shadow": "{sys.elevation.shadow.2}",
              "disabled": {
                "text": "{sys.palette.onSurface}",
                "textOpacity": "{sys.opacity.surface.disabled}",
                "backgroundOpacity": "{sys.opacity.disabled}"
              },
              "active": {
                "shadow": "{sys.elevation.shadow.8}"
              }
            }
          },
          "elevated": {
            "var|sys": {
              "backgroundOpacity": "{sys.opacity.overlay}",
              "shadow": "{sys.elevation.shadow.2}",
              "disabled": {
                "text": "{sys.palette.onSurface}",
                "textOpacity": "{sys.opacity.surface.disabled}",
                "backgroundOpacity": "{sys.opacity.disabled}"
              },
              "active": {
                "shadow": "{sys.elevation.shadow.8}"
              }
            }
          },
          "tonal": {
            "var|sys": {
              "text": "{sys.palette.onSurface}",
              "backgroundOpacity": "{sys.opacity.overlay}",
              "frame": {
                "border": "{??sys.palette}"
              },
              "disabled": {
                "text": "{sys.palette.onSurface}",
                "textOpacity": "{sys.opacity.surface.disabled}",
                "backgroundOpacity": "{sys.opacity.disabled}"
              },
              "active": {
                "frame": {
                  "borderOpacity": "{sys.opacity.surface.medium}"
                }
              }
            }
          },
          "outlined": {
            "var|sys": {
              "border": "{sys.palette.onSurface}",
              "borderOpacity": "{sys.opacity.border}",
              "frame": {
                "border": "{??sys.palette}"
              },
              "disabled": {
                "textOpacity": "{sys.opacity.surface.disabled}"
              },
              "active": {
                "frame": {
                  "borderOpacity": "{sys.opacity.surface.medium}"
                }
              }
            }
          },
          "text": {
            "var|sys": {
              "padding": "{ref.spacing.2}",
              "icon": {
                "margin": "{ref.spacing.1}"
              },
              "frame": {
                "border": "{??sys.palette}"
              },
              "dense": {
                "padding": "{ref.spacing.1}",
                "icon": {
                  "margin": "{ref.spacing.px} * 2"
                }
              },
              "disabled": {
                "textOpacity": "{sys.opacity.surface.disabled}"
              },
              "active": {
                "frame": {
                  "borderOpacity": "{sys.opacity.surface.medium}"
                }
              }
            }
          }
        }
      },
      "@font": "{font.button}",
      "@@palette": "{?palette}",
      "height": {
        "sm": "{ref.spacing.8}",
        "md": "{ref.spacing.9}",
        "lg": "{ref.spacing.11}"
      },
      "width": {
        "sm": "{ref.spacing.20}",
        "md": "{ref.spacing.28}",
        "lg": "{ref.spacing.36}"
      },
      "~appearance": {
        "contained": {
          "@style": "{??m.appearance.contained}"
        },
        "elevated": {
          "@style": "{??m.appearance.elevated}"
        },
        "tonal": {
          "@style": "{??m.appearance.tonal}"
        },
        "outlined": {
          "@style": "{??m.appearance.outlined}"
        },
        "text": {
          "@style": "{??m.appearance.text}"
        },
        "default": "contained"
      },
      "~adaptive": {
        "sm": {
          "media|{ref.screen.sm}": {
            "@style": "{??m.text}"
          }
        },
        "md": {
          "media|{ref.screen.md}": {
            "@style": "{??m.text}"
          }
        },
        "lg": {
          "media|{ref.screen.lg}": {
            "@style": "{??m.text}"
          }
        }
      },
      "~rounded": {
        "@style": "{?rounded}",
        "full": {
          "@style": "{??m.rounded.full}"
        }
      },
      "~lowercase": {
        "@style": "{font.body1}"
      }
    }
  }
}
