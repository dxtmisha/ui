{
  "md2": {
    "fab": {
      "@style": "{d.fab}",
      "var|sys": {
        "palette": "{sys.palette.primary}",
        "padding": "{ref.spacing.5}",
        "space": "{ref.spacing.1}",
        "width": "auto",
        "height": "{ref.spacing.12}",
        "zIndex": "{sys.elevation.index.6}",
        "borderWidth": "{ref.spacing.px}",
        "icon": {
          "padding": "{ref.spacing.1}",
          "margin": "{ref.spacing.3}",
          "fontSize": "{ref.font.size.2xl}"
        },
        "iconTrailing": {
          "fontSize": "{ref.font.size.xl}"
        },
        "dense": {
          "padding": "{ref.spacing.2}"
        },
        "selected": {
          "palette": "{sys.palette.primaryVariant}"
        },
        "disabled": {
          "palette": "{sys.palette.disabled}",
          "shadow": "none"
        },
        "hover": {
          "gradientOpacity": "{sys.opacity.hover}"
        },
        "focus": {
          "gradientOpacity": "{sys.opacity.focus}"
        },
        "active": {
          "gradientOpacity": "{sys.opacity.dragged}"
        },
        "dragged": {
          "shadow": "{sys.elevation.shadow.8}"
        }
      },
      "mixin|m": {
        "appearance": {
          "contained": {
            "var|sys": {
              "text": "{sys.palette.onPrimary}",
              "textOpacity": "{sys.opacity.primary.high}",
              "backgroundOpacity": "{sys.opacity.primary.high}",
              "gradient": "{sys.palette.onPrimary}",
              "shadow": "{sys.elevation.shadow.6}",
              "disabled": {
                "text": "{sys.palette.onSurface}",
                "textOpacity": "{sys.opacity.surface.disabled}",
                "backgroundOpacity": "{sys.opacity.disabled}"
              },
              "active": {
                "shadow": "{sys.elevation.shadow.12}"
              }
            }
          }
        }
      },
      "@font": "{font.button}",
      "@ellipsis": "{d.font.ellipsis}",
      "@@palette": "{?palette}",
      "@@appearance": "{??m.appearance.contained}",
      "width": {
        "sm": "{ref.spacing.24}",
        "md": "{ref.spacing.32}",
        "lg": "{ref.spacing.40}"
      },
      "height": {
        "sm": "{ref.spacing.10}",
        "md": "{ref.spacing.12}",
        "lg": "{ref.spacing.14}"
      },
      "~adaptive": {
        "sm": {
          "media|{ref.screen.sm}": {
            "@style": "{??m.text}"
          }
        },
        "md": {
          "media|{ref.screen.md}": {
            "@style": "{??m.text}"
          }
        },
        "lg": {
          "media|{ref.screen.lg}": {
            "@style": "{??m.text}"
          }
        }
      },
      "~rounded": {
        "@style": "{?rounded}",
        "full": {
          "@style": "{??m.rounded.full}"
        }
      }
    }
  }
}
