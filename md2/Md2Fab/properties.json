{
  "md2": {
    "fab": {
      "@style": "{d.fab}",
      "var|sys": {
        "var|palette": "{sys.palette.primary}",
        "var|backgroundOpacity": "{sys.opacity.primary.high}",
        "var|borderWidth": "{ref.spacing.px}",
        "var|dense": "{ref.spacing.2}",
        "var|gradient": "{sys.palette.onPrimary}",
        "var|padding": "{ref.spacing.5}",
        "var|shadow": "{sys.elevation.shadow.6}",
        "var|space": "{ref.spacing.1}",
        "var|text": "{sys.palette.onPrimary}",
        "var|textOpacity": "{sys.opacity.primary.high}",
        "var|width": "auto",
        "var|active": {
          "var|gradientOpacity": "{sys.opacity.dragged}",
          "var|shadow": "{sys.elevation.shadow.12}"
        },
        "var|disabled": {
          "var|palette": "{sys.palette.disabled}",
          "var|backgroundOpacity": "{sys.opacity.disabled}",
          "var|shadow": "none",
          "var|text": "{sys.palette.onSurface}",
          "var|textOpacity": "{sys.opacity.surface.disabled}"
        },
        "var|dragged": {
          "var|shadow": "{sys.elevation.shadow.8}"
        },
        "var|focus": {
          "var|gradientOpacity": "{sys.opacity.focus}"
        },
        "var|hover": {
          "var|gradientOpacity": "{sys.opacity.hover}"
        },
        "var|selected": {
          "var|palette": "{sys.palette.primaryVariant}"
        },
        "var|icon": {
          "var|margin": "{ref.spacing.3}",
          "var|padding": "{ref.spacing.1}",
          "var|text": "{ref.font.size.2xl}",
          "var|textTrailing": "{ref.font.size.xl}"
        }
      },
      "@font": "{font.button}",
      "@round": "{??m.round}",
      "border-radius": "{sys.rounded.full}",
      "height": {
        "sm": "{ref.spacing.10}",
        "md": "{ref.spacing.12}",
        "lg": "{ref.spacing.14}",
        "default": "md"
      },
      "gradient": "{??l.gradient}",
      "z-index": "{sys.elevation.index.6}",
      "@@palette": "{?palette}",
      "section|adaptive": {
        "sm": {
          "media|{ref.screen.sm}": {
            "@style": "{??m.text}"
          }
        },
        "md": {
          "media|{ref.screen.md}": {
            "@style": "{??m.text}"
          }
        },
        "lg": {
          "media|{ref.screen.lg}": {
            "@style": "{??m.text}"
          }
        },
        "full": {
          "@style": "{??m.text}"
        },
        "icon": {
          "--": "none"
        },
        "default": "full"
      },
      "section|lowercase": {
        "@font": "{font.body1}"
      },
      "section|rounded": {
        "@style": "{?rounded}"
      }
    }
  }
}
