{
  "md2": {
    "icon": {
      "@style": "{d.icon}",
      "var|_background": "{?sys.palette.onSurface}",
      "var|_disabled": "{?sys.opacity.disabled}",
      "var|_opacity": "{?sys.opacity.dragged}",
      "var|_rounded": "{?.sys.rounded.full}",
      "var|_size": "{?.ref.spacing.6}",
      "size": {
        "md": {
          "var|?image-fontSize": "{ref.font.size.headline.5}",
          "var|_size": "{?.ref.spacing.10}"
        },
        "lg": {
          "var|?image-fontSize": "{ref.font.size.headline.4}",
          "var|_rounded": "{?.sys.rounded.standard}",
          "var|_size": "{?.ref.spacing.14}"
        },
        "xl": {
          "var|?image-fontSize": "{ref.font.size.headline.3}",
          "var|_rounded": "{?.sys.rounded.none}",
          "var|_size": "{?.ref.spacing.25}"
        }
      }
    }
  }
}
