{
  "d": {
    "checkbox": {
      "var|sys": {
        "palette": "",
        "gap": "",
        "text": "",
        "icon": {
          "backgroundOpacity": "0",
          "borderOpacity": "",
          "borderWidth": "",
          "rounded": "",
          "size": "",
          "text": "",
          "textOpacity": ""
        },
        "item": {
          "backgroundOpacity": "0",
          "borderOpacity": "0",
          "borderWidth": "",
          "size": ""
        },
        "label": {
          "lineHeight": "",
          "required": ""
        },
        "active": {
          "item": {
            "backgroundOpacity": "0",
            "borderOpacity": "0"
          }
        },
        "hover": {
          "item": {
            "backgroundOpacity": "0",
            "borderOpacity": "0"
          }
        },
        "checked": {
          "palette": "",
          "icon": {
            "backgroundOpacity": "{??sys.icon.textOpacity}",
            "borderOpacity": "{??sys.icon.textOpacity}",
            "text": "{??sys.icon.text}",
            "textOpacity": "{??sys.icon.textOpacity}"
          }
        },
        "error": {
          "palette": ""
        }
      },
      "var|l": {
        "palette": "{??sys.palette}",
        "text": "{??sys.text}",
        "icon": {
          "backgroundOpacity": "{??sys.icon.backgroundOpacity}",
          "borderOpacity": "{??sys.icon.borderOpacity}",
          "text": "{??sys.icon.text}",
          "textOpacity": "{??sys.icon.textOpacity}"
        },
        "item": {
          "backgroundOpacity": "{??sys.item.backgroundOpacity}",
          "borderOpacity": "{??sys.item.borderOpacity}"
        }
      },
      "#icon": {},
      "#input": {},
      "#item": {},
      "#required": {},
      "#right": {},
      "#text": {},
      "& .?field-message": {
        "padding-top": "0"
      }
    }
  }
}
