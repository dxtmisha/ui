{
  "d": {
    "checkbox": {
      "var|sys": {
        "palette": "",
        "gap": "",
        "text": "",
        "textOpacity": "",
        "item": {
          "size": "",
          "backgroundOpacity": "0",
          "borderOpacity": "0",
          "borderWidth": ""
        },
        "icon": {
          "size": "",
          "rounded": "",
          "text": "",
          "textOpacity": "",
          "backgroundOpacity": "0",
          "borderOpacity": "",
          "borderWidth": "",
          "clipPath": "inset(0 100% 0 0)",
          "transitionProperty": "none"
        },
        "label": {
          "lineHeight": ""
        },
        "required": {
          "text": ""
        },
        "checked": {
          "palette": "",
          "icon": {
            "backgroundOpacity": "{??sys.icon.textOpacity}",
            "borderOpacity": "0",
            "text": "{??sys.icon.text}",
            "textOpacity": "{??sys.icon.textOpacity}",
            "clipPath": "inset(0 0 0 0)",
            "transitionProperty": "clip-path, opacity, transform"
          }
        },
        "error": {
          "palette": "",
          "text": "{??sys.error.palette}"
        },
        "disabled": {
          "palette": "",
          "textOpacity": "",
          "item": {
            "backgroundOpacity": "0",
            "borderOpacity": "0"
          },
          "icon": {
            "borderOpacity": "{??sys.disabled.textOpacity}"
          },
          "checked": {
            "icon": {
              "backgroundOpacity": "{??sys.disabled.textOpacity}",
              "borderOpacity": "0"
            }
          }
        },
        "hover": {
          "item": {
            "backgroundOpacity": "0",
            "borderOpacity": "0"
          }
        },
        "active": {
          "item": {
            "backgroundOpacity": "0",
            "borderOpacity": "0"
          }
        }
      },
      "var|l": {
        "palette": "{??sys.palette}",
        "text": "{??sys.text}",
        "textOpacity": "{??sys.textOpacity}",
        "item": {
          "backgroundOpacity": "{??sys.item.backgroundOpacity}",
          "borderOpacity": "{??sys.item.borderOpacity}"
        },
        "icon": {
          "text": "{??sys.icon.text}",
          "textOpacity": "{??sys.icon.textOpacity}",
          "backgroundOpacity": "{??sys.icon.backgroundOpacity}",
          "borderOpacity": "{??sys.icon.borderOpacity}",
          "clipPath": "{??sys.icon.clipPath}",
          "transitionProperty": "{??sys.icon.transitionProperty}"
        }
      },
      "#circle": {},
      "#disabled": {},
      "#icon": {},
      "#input": {},
      "#item": {
        "?ripple-background": "{??l.palette}"
      },
      "#required": {},
      "#right": {},
      "#switch": {
        "?ripple-background": "{??l.palette}"
      },
      "#text": {},
      "& .?field-message": {
        "padding-top": "0"
      }
    }
  }
}
