{
  "d": {
    "button": {
      "var|sys": {
        "palette": "",
        "padding": "",
        "space": "",
        "height": "",
        "width": "auto",
        "zIndex": "",
        "text": "",
        "textOpacity": "",
        "background": "{??sys.palette}",
        "backgroundOpacity": "0",
        "gradient": "{??sys.palette}",
        "gradientOpacity": "0",
        "border": "{??sys.palette}",
        "borderOpacity": "0",
        "borderWidth": "",
        "shadow": "none",
        "icon": {
          "padding": "",
          "margin": "",
          "fontSize": ""
        },
        "iconTrailing": {
          "fontSize": ""
        },
        "dense": {
          "padding": ""
        },
        "selected": {
          "palette": "{??sys.palette}",
          "background": "{??sys.selected.palette}",
          "gradient": "{??sys.selected.palette}",
          "border": "{??sys.selected.palette}"
        },
        "disabled": {
          "palette": "",
          "background": "{??sys.disabled.palette}",
          "gradient": "{??sys.disabled.palette}",
          "border": "{??sys.disabled.palette}"
        },
        "hover": {},
        "focus": {},
        "active": {},
        "dragged": {}
      },
      "var|l": {
        "palette": "{??sys.palette}",
        "padding": "{??sys.padding}",
        "height": "{??sys.height}",
        "width": "{??sys.width}",
        "zIndex": "{??sys.zIndex}",
        "text": "{??sys.text}",
        "textOpacity": "{??sys.textOpacity}",
        "background": "{??sys.background}",
        "backgroundOpacity": "{??sys.backgroundOpacity}",
        "gradient": "{??sys.gradient}",
        "gradientOpacity": "{??sys.gradientOpacity}",
        "border": "{??sys.border}",
        "borderOpacity": "{??sys.borderOpacity}",
        "borderWidth": "{??sys.borderWidth}",
        "shadow": "{??sys.shadow}",
        "icon": {
          "margin": "{??sys.icon.margin}",
          "space": "{??sys.icon.space}"
        },
        "document": {
          "display": "none"
        }
      },
      "mixin|m": {
        "appearance": {
          "contained": {
            "var|sys": {
              "text": "",
              "textOpacity": "",
              "backgroundOpacity": "",
              "gradient": "",
              "shadow": "",
              "selected": {
                "gradient": "{??sys.gradient}"
              },
              "disabled": {
                "text": "{??sys.text}",
                "textOpacity": "{??sys.textOpacity}",
                "backgroundOpacity": "{??sys.backgroundOpacity}",
                "shadow": "{??sys.shadow}"
              },
              "active": {
                "shadow": "{??sys.shadow}"
              }
            },
            "gradient": "{??l.gradient}"
          }
        },
        "mixin|border": {
          "@position": "{d.position.absolute}",
          "border-opacity": "{??l.borderOpacity}",
          "border-width": "{??l.borderWidth}",
          "palette-border": "{??l.border}"
        },
        "mixin|icon": {
          "var|l": {
            "padding": "{??sys.icon.padding}",
            "width": "var(--sys-height)",
            "icon": {
              "margin": "0",
              "space": "0"
            }
          }
        },
        "mixin|text": {
          "var|l": {
            "padding": "{??sys.padding}",
            "width": "{??sys.width}",
            "icon": {
              "margin": "{??sys.icon.margin} - {??l.padding}",
              "space": "{??sys.space}"
            },
            "document": {
              "display": "block"
            }
          }
        },
        "mixin|before": {
          "::before": {
            "@style": "{??m.border}",
            "transition-duration": "{d.sys.transitionDuration.fade.through}",
            "transition-property": "border-color"
          },
          ":active": {
            "::before": {
              "transition-duration": "{d.sys.transitionDuration.sm}"
            }
          }
        },
        "mixin|round": {
          "var|sys": {
            "icon": {
              "margin": "{??sys.icon.padding}"
            }
          },
          "& .?icon": {
            "var|?icon-size": "var(--sys-height) - ({??sys.icon.padding} * 2)"
          }
        }
      },
      "@style": "{??m.icon}",
      "@@ellipsis": "{d.font.ellipsis}",
      "width": {
        "auto": "auto",
        "full": "100%",
        "rename": "var|??l.width"
      },
      "height": {
        "auto": "auto",
        "rename": "var|??l.height"
      },
      "#text": {},
      "&.is-icon": {
        "@style": "{??m.icon}"
      },
      "~adaptive": {
        "full": {
          "@style": "{??m.text}"
        },
        "icon": {},
        "default": "full"
      },
      "~dense": {
        "var|l": {
          "padding": "{??sys.dense.padding}"
        }
      },
      "~align": {
        "@style": "{d.font.align}"
      },
      "~selected": {},
      "~readonly": {},
      "~progress": {},
      "~focus": {},
      "~dragged": {}
    }
  }
}
