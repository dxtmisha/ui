{
  "d": {
    "button": {
      "mixin|m": {
        "mixin|status": {
          "var|palette": "{??sys.palette}",
          "var|background": "{??sys.background}",
          "var|backgroundOpacity": "{??sys.backgroundOpacity}",
          "var|border": "{??sys.border}",
          "var|borderOpacity": "{??sys.borderOpacity}",
          "var|gradient": "{??sys.gradient}",
          "var|gradientOpacity": "{??sys.gradientOpacity}",
          "var|shadow": "{??sys.shadow}",
          "var|text": "{??sys.text}",
          "var|textOpacity": "{??sys.textOpacity}"
        },
        "mixin|border": {
          "@position": "{d.position.absolute}",
          "border-opacity": "{??l.borderOpacity}",
          "border-width": "{??l.borderWidth}",
          "palette-border": "{??l.border}"
        },
        "mixin|before": {
          "::before": {
            "@style": "{??m.border}",
            "transition-duration": "{d.sys.transitionDuration.fade.through}",
            "transition-property": "border-color"
          },
          ":active": {
            "::before": {
              "transition-duration": "{d.sys.transitionDuration.sm}"
            }
          }
        },
        "mixin|icon": {
          "var|l": {
            "padding": "{??sys.icon.padding}",
            "width": "var(--sys-height)",
            "icon": {
              "margin": "0px",
              "space": "0px"
            }
          }
        },
        "mixin|round": {
          "var|sys": {
            "icon": {
              "margin": "{??sys.icon.padding}"
            }
          },
          "& .?icon": {
            "var|?icon-size": "var(--sys-height) - ({??sys.icon.padding} * 2)"
          }
        },
        "mixin|text": {
          "var|l": {
            "textDisplay": "block",
            "padding": "{??sys.padding}",
            "width": "{??sys.width}",
            "icon": {
              "margin": "{??sys.icon.margin} - {??sys.padding}",
              "space": "{??sys.space}"
            }
          }
        },
        "mixin|zIndex": {
          "z-index": "4"
        },
        "mixin|disabled": {
          "cursor": "default",
          "z-index": "auto"
        }
      },
      "var|sys": {
        "palette": "#000000",
        "background": "{??sys.palette}",
        "backgroundOpacity": "0",
        "border": "{??sys.palette}",
        "borderOpacity": "0",
        "borderWidth": "1px",
        "dense": "8px",
        "gradient": "{??sys.palette}",
        "gradientOpacity": "0",
        "padding": "16px",
        "shadow": "none",
        "space": "4px",
        "text": "unset",
        "textOpacity": "unset",
        "width": "80px",
        "active": {
          "@style": "{??m.status}",
          "gradientOpacity": "0.08"
        },
        "dragged": {
          "@style": "{??m.status}"
        },
        "disabled": {
          "@style": "{??m.status}",
          "background": "{??sys.disabled.palette}",
          "border": "{??sys.disabled.palette}",
          "gradient": "{??sys.disabled.palette}"
        },
        "focus": {
          "@style": "{??m.status}",
          "gradientOpacity": "0.12"
        },
        "hover": {
          "@style": "{??m.status}",
          "gradientOpacity": "0.04"
        },
        "selected": {
          "@style": "{??m.status}",
          "background": "{??sys.selected.palette}",
          "border": "{??sys.selected.palette}"
        },
        "icon": {
          "margin": "12px",
          "padding": "4px",
          "text": "20px",
          "textTrailing": "18px"
        }
      },
      "var|l": {
        "palette": "{??sys.palette}",
        "align": "center",
        "background": "{??sys.background}",
        "backgroundOpacity": "{??sys.backgroundOpacity}",
        "border": "{??sys.border}",
        "borderOpacity": "{??sys.borderOpacity}",
        "borderWidth": "{??sys.borderWidth}",
        "gradient": "{??sys.gradient}",
        "gradientOpacity": "{??sys.gradientOpacity}",
        "shadow": "{??sys.shadow}",
        "text": "{??sys.text}",
        "textOpacity": "{??sys.textOpacity}",
        "textDisplay": "none"
      },
      "var|?image-font-size": "{??sys.icon.text}",
      "box-shadow": "{??l.shadow}",
      "cursor": "pointer",
      "max-width": "100%",
      "min-width": "{??l.width}",
      "padding-x": "{??l.padding}",
      "position": "relative",
      "text-align": "center",
      "transition-property": "color, background, box-shadow",
      "section|align": {
        "@style": "{d.font.align}"
      },
      "section|dense": {
        "var|sys": {
          "padding": "{??sys.dense}"
        },
        "min-width": "auto"
      },
      "section|ellipsis": {
        "@style": "{d.font.ellipsis}",
        "default": true
      },
      "section|lowercase": {
        "text-transform": "none"
      },
      "section|dragged": {
        "@style": "{??m.zIndex}",
        "var|l": {
          "palette": "{??sys.dragged.palette}",
          "background": "{??sys.dragged.background}",
          "backgroundOpacity": "{??sys.dragged.backgroundOpacity} !important",
          "border": "{??sys.dragged.border}",
          "borderOpacity": "{??sys.dragged.borderOpacity} !important",
          "gradient": "{??sys.dragged.gradient}",
          "gradientOpacity": "{??sys.dragged.gradientOpacity} !important",
          "shadow": "{??sys.dragged.shadow} !important",
          "text": "{??sys.dragged.text}",
          "textOpacity": "{??sys.dragged.textOpacity}"
        }
      },
      "section|readonly": {
        "@style": "{??m.disabled}",
        "var|l": {
          "palette": "{??sys.palette} !important",
          "background": "{??sys.background} !important",
          "backgroundOpacity": "{??sys.backgroundOpacity} !important",
          "border": "{??sys.border} !important",
          "borderOpacity": "{??sys.borderOpacity} !important",
          "gradient": "{??sys.gradient} !important",
          "gradientOpacity": "{??sys.gradientOpacity} !important",
          "shadow": "{??sys.shadow} !important",
          "text": "{??sys.text} !important",
          "textOpacity": "{??sys.textOpacity} !important"
        },
        "cursor": "default"
      },
      "section|selected": {
        "var|l": {
          "palette": "{??sys.selected.palette} !important",
          "background": "{??sys.selected.background} !important",
          "backgroundOpacity": "{??sys.selected.backgroundOpacity}",
          "border": "{??sys.selected.border} !important",
          "borderOpacity": "{??sys.selected.borderOpacity}",
          "gradient": "{??sys.selected.gradient} !important",
          "gradientOpacity": "{??sys.selected.gradientOpacity}",
          "shadow": "{??sys.selected.shadow}",
          "text": "{??sys.selected.text} !important",
          "textOpacity": "{??sys.selected.textOpacity}"
        }
      },
      "section|disabled": {
        "@style": "{??m.disabled}",
        "var|l": {
          "palette": "{??sys.disabled.palette} !important",
          "background": "{??sys.disabled.background} !important",
          "backgroundOpacity": "{??sys.disabled.backgroundOpacity} !important",
          "border": "{??sys.disabled.border} !important",
          "borderOpacity": "{??sys.disabled.borderOpacity} !important",
          "gradient": "{??sys.disabled.gradient} !important",
          "gradientOpacity": "{??sys.disabled.gradientOpacity} !important",
          "shadow": "{??sys.disabled.shadow} !important",
          "text": "{??sys.disabled.text} !important",
          "textOpacity": "{??sys.disabled.textOpacity} !important"
        },
        "background-color": "{??l.background}",
        "gradient-color": "{??l.gradient}"
      },
      "#text": {
        "@dynamic": "{d.flex.dynamic}",
        "align-items": "center",
        "display": "{??l.textDisplay}",
        "max-width": "inherit",
        "overflow": "inherit",
        "text-overflow": "inherit",
        "transition-property": "color",
        "white-space": "inherit"
      },
      "& .?icon": {
        "margin-left": "{??l.icon.margin}",
        "margin-right": "{??l.icon.space}",
        "section|inEnd": {
          "var|?image-font-size": "{??sys.icon.textTrailing}",
          "margin-left": "{??l.icon.space}",
          "margin-right": "{??l.icon.margin}",
          "position": "relative",
          "z-index": "4"
        }
      },
      "& .?progress.is-visible~.??__text": {
        "color": "transparent"
      },
      "& .?progress.is-visible~.?icon": {
        "opacity": "0"
      },
      "=isIcon": {
        "@style": "{??m.icon}"
      },
      ":hover": {
        "@style": "{??m.zIndex}",
        "var|l": {
          "palette": "{??sys.hover.palette}",
          "background": "{??sys.hover.background}",
          "backgroundOpacity": "{??sys.hover.backgroundOpacity}",
          "border": "{??sys.hover.border}",
          "borderOpacity": "{??sys.hover.borderOpacity}",
          "gradient": "{??sys.hover.gradient}",
          "gradientOpacity": "{??sys.hover.gradientOpacity}",
          "shadow": "{??sys.hover.shadow}",
          "text": "{??sys.hover.text}",
          "textOpacity": "{??sys.hover.textOpacity}"
        }
      },
      ":focus": {
        "@style": "{??m.zIndex}",
        "var|l": {
          "palette": "{??sys.focus.palette}",
          "background": "{??sys.focus.background}",
          "backgroundOpacity": "{??sys.focus.backgroundOpacity}",
          "border": "{??sys.focus.border}",
          "borderOpacity": "{??sys.focus.borderOpacity}",
          "gradient": "{??sys.focus.gradient}",
          "gradientOpacity": "{??sys.focus.gradientOpacity}",
          "shadow": "{??sys.focus.shadow}",
          "text": "{??sys.focus.text}",
          "textOpacity": "{??sys.focus.textOpacity}"
        }
      },
      ":active": {
        "@style": "{??m.zIndex}",
        "var|l": {
          "palette": "{??sys.active.palette}",
          "background": "{??sys.active.background}",
          "backgroundOpacity": "{??sys.active.backgroundOpacity}",
          "border": "{??sys.active.border}",
          "borderOpacity": "{??sys.active.borderOpacity}",
          "gradient": "{??sys.active.gradient}",
          "gradientOpacity": "{??sys.active.gradientOpacity}",
          "shadow": "{??sys.active.shadow}",
          "text": "{??sys.active.text}",
          "textOpacity": "{??sys.active.textOpacity}"
        }
      }
    }
  }
}
