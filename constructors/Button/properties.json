{
  "d": {
    "sys": {
      "mixin|button": {
        "mixin|status": {
          "var|palette": "{?sys.button.palette}",
          "var|background": "{?sys.button.background}",
          "var|backgroundOpacity": "{?sys.button.backgroundOpacity}",
          "var|border": "{?sys.button.border}",
          "var|borderOpacity": "{?sys.button.borderOpacity}",
          "var|gradient": "{?sys.button.gradient}",
          "var|gradientOpacity": "{?sys.button.gradientOpacity}",
          "var|shadow": "{?sys.button.shadow}",
          "var|text": "{?sys.button.text}",
          "var|textOpacity": "{?sys.button.textOpacity}"
        },
        "var|palette": "#000000",
        "var|background": "{??_palette}",
        "var|backgroundOpacity": "0",
        "var|border": "{??_palette}",
        "var|borderOpacity": "0",
        "var|borderWidth": "1px",
        "var|gradient": "{??_palette}",
        "var|gradientOpacity": "0",
        "var|padding": "16px",
        "var|shadow": "none",
        "var|text": "unset",
        "var|textOpacity": "unset",
        "var|width": "80px",
        "var|active": {
          "@style": "{d.sys.button.status}",
          "var|gradientOpacity": "0.08"
        },
        "var|disabled": {
          "@style": "{d.sys.button.status}"
        },
        "var|focus": {
          "@style": "{d.sys.button.status}",
          "var|gradientOpacity": "0.12"
        },
        "var|hover": {
          "@style": "{d.sys.button.status}",
          "var|gradientOpacity": "0.04"
        },
        "var|selected": {
          "@style": "{d.sys.button.status}"
        }
      }
    },
    "button": {
      "var|palette": "{?sys.button.palette}",
      "var|background": "{?sys.button.background}",
      "var|backgroundOpacity": "{?sys.button.backgroundOpacity}",
      "var|border": "{?sys.button.border}",
      "var|borderOpacity": "{?sys.button.borderOpacity}",
      "var|gradient": "{?sys.button.gradient}",
      "var|gradientOpacity": "{?sys.button.gradientOpacity}",
      "var|padding": "{?sys.button.padding}",
      "var|shadow": "{?sys.button.shadow}",
      "var|text": "{?sys.button.text}",
      "var|textOpacity": "{?sys.button.textOpacity}",
      "var|width": "{?sys.button.width}",
      "mixin|border": {
        "@position": "{d.position.absolute}",
        "border-opacity": "{??borderOpacity}",
        "border-width": "{??borderWidth}",
        "palette-border": "{??border}"
      },
      "mixin|zIndex": {
        "z-index": "4"
      },
      "mixin|before": {
        "@style": "{??border}",
        "transition-duration": "{d.sys.transitionDuration.fade.through}",
        "transition-property": "border-color",
        ":active": {
          "transition-duration": "{d.sys.transitionDuration.xs}"
        }
      },
      "mixin|active": {
        "@style": "{??zIndex}",
        "var|palette": "{?sys.button.active.palette}",
        "var|background": "{?sys.button.active.background}",
        "var|backgroundOpacity": "{?sys.button.active.backgroundOpacity}",
        "var|border": "{?sys.button.active.border}",
        "var|borderOpacity": "{?sys.button.active.borderOpacity}",
        "var|gradient": "{?sys.button.active.gradient}",
        "var|gradientOpacity": "{?sys.button.active.gradientOpacity}",
        "var|shadow": "{?sys.button.active.shadow}",
        "var|text": "{?sys.button.active.text}",
        "var|textOpacity": "{?sys.button.active.textOpacity}"
      },
      "mixin|disabled": {
        "cursor": "default",
        "z-index": "auto"
      },
      "@flex": "{d.flex.inline.center}",
      "@select": "{d.font.readonly}",
      "background-opacity": "{??backgroundOpacity}",
      "box-shadow": "{??shadow}",
      "color": "{??text}",
      "color-opacity": "{??textOpacity}",
      "gradient-opacity": "{??gradientOpacity}",
      "max-width": "100%",
      "min-width": "{??width}",
      "padding-x": "{??padding}",
      "palette-background": "{??background}",
      "palette-gradient": "{??gradient}",
      "position": "relative",
      "transition-property": "color, background, box-shadow",
      ":hover": {
        "@style": "{??zIndex}",
        "var|palette": "{?sys.button.hover.palette}",
        "var|background": "{?sys.button.hover.background}",
        "var|backgroundOpacity": "{?sys.button.hover.backgroundOpacity}",
        "var|border": "{?sys.button.hover.border}",
        "var|borderOpacity": "{?sys.button.hover.borderOpacity}",
        "var|gradient": "{?sys.button.hover.gradient}",
        "var|gradientOpacity": "{?sys.button.hover.gradientOpacity}",
        "var|shadow": "{?sys.button.hover.shadow}",
        "var|text": "{?sys.button.hover.text}",
        "var|textOpacity": "{?sys.button.hover.textOpacity}"
      },
      ":focus": {
        "@style": "{??zIndex}",
        "var|palette": "{?sys.button.focus.palette}",
        "var|background": "{?sys.button.focus.background}",
        "var|backgroundOpacity": "{?sys.button.focus.backgroundOpacity}",
        "var|border": "{?sys.button.focus.border}",
        "var|borderOpacity": "{?sys.button.focus.borderOpacity}",
        "var|gradient": "{?sys.button.focus.gradient}",
        "var|gradientOpacity": "{?sys.button.focus.gradientOpacity}",
        "var|shadow": "{?sys.button.focus.shadow}",
        "var|text": "{?sys.button.focus.text}",
        "var|textOpacity": "{?sys.button.focus.textOpacity}"
      },
      ":active": {
        "@style": "{??active}"
      },
      "section|selected": {
        "var|palette": "{?sys.button.selected.palette}",
        "var|background": "{?sys.button.selected.background}",
        "var|backgroundOpacity": "{?sys.button.selected.backgroundOpacity}",
        "var|border": "{?sys.button.selected.border}",
        "var|borderOpacity": "{?sys.button.selected.borderOpacity}",
        "var|gradient": "{?sys.button.selected.gradient}",
        "var|gradientOpacity": "{?sys.button.selected.gradientOpacity}",
        "var|shadow": "{?sys.button.selected.shadow}",
        "var|text": "{?sys.button.selected.text}",
        "var|textOpacity": "{?sys.button.selected.textOpacity}"
      },
      "section|dragged": {
        "@style": "{??active}"
      },
      "section|readonly": {
        "@style": "{??disabled}",
        "var|palette": "{?sys.button.palette}",
        "var|background": "{?sys.button.background}",
        "var|backgroundOpacity": "{?sys.button.backgroundOpacity}",
        "var|border": "{?sys.button.border}",
        "var|borderOpacity": "{?sys.button.borderOpacity}",
        "var|gradient": "{?sys.button.gradient}",
        "var|gradientOpacity": "{?sys.button.gradientOpacity}",
        "var|shadow": "{?sys.button.shadow}",
        "var|text": "{?sys.button.text}",
        "var|textOpacity": "{?sys.button.textOpacity}"
      },
      "section|disabled": {
        "@style": "{??disabled}",
        "var|palette": "{?sys.button.disabled.palette}",
        "var|background": "{?sys.button.disabled.background}",
        "var|backgroundOpacity": "{?sys.button.disabled.backgroundOpacity}",
        "var|border": "{?sys.button.disabled.border}",
        "var|borderOpacity": "{?sys.button.disabled.borderOpacity}",
        "var|gradient": "{?sys.button.disabled.gradient}",
        "var|gradientOpacity": "{?sys.button.disabled.gradientOpacity}",
        "var|shadow": "{?sys.button.disabled.shadow}",
        "var|text": "{?sys.button.disabled.text}",
        "var|textOpacity": "{?sys.button.disabled.textOpacity}",
        "background-color": "{??background}",
        "gradient-color": "{??gradient}"
      },
      "section|align": {
        "@style": "{d.font.align}"
      },
      "#text": {
        "@dynamic": "{d.flex.dynamic}"
      }
    }
  }
}
