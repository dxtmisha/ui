{
  "d": {
    "button": {
      "var|sys": {
        "palette": "",
        "padding": "",
        "space": "",
        "height": "",
        "width": "auto",
        "zIndex": "",
        "text": "",
        "textOpacity": "",
        "background": "{??sys.palette}",
        "backgroundOpacity": "0",
        "gradient": "{??sys.palette}",
        "gradientOpacity": "0",
        "border": "{??sys.palette}",
        "borderOpacity": "0",
        "borderWidth": "",
        "rounded": "",
        "shadow": "none",
        "icon": {
          "padding": "",
          "margin": "",
          "fontSize": ""
        },
        "iconTrailing": {
          "fontSize": ""
        },
        "frame": {
          "border": "{??sys.border}",
          "borderOpacity": "0",
          "borderWidth": "{??sys.borderWidth}"
        },
        "dense": {
          "padding": "",
          "icon": {
            "margin": ""
          }
        },
        "selected": {
          "palette": "{??sys.palette}",
          "background": "{??sys.selected.palette}",
          "gradient": "{??sys.selected.palette}",
          "border": "{??sys.selected.palette}"
        },
        "disabled": {
          "palette": "",
          "background": "{??sys.disabled.palette}",
          "gradient": "{??sys.disabled.palette}",
          "border": "{??sys.disabled.palette}"
        },
        "hover": {},
        "focus": {},
        "active": {},
        "dragged": {}
      },
      "var|l": {
        "palette": "{??sys.palette}",
        "padding": "{??sys.padding}",
        "width": "{??sys.width}",
        "zIndex": "{??sys.zIndex}",
        "text": "{??sys.text}",
        "textOpacity": "{??sys.textOpacity}",
        "background": "{??sys.background}",
        "backgroundOpacity": "{??sys.backgroundOpacity}",
        "gradient": "{??sys.gradient}",
        "gradientOpacity": "{??sys.gradientOpacity}",
        "border": "{??sys.border}",
        "borderOpacity": "{??sys.borderOpacity}",
        "borderWidth": "{??sys.borderWidth}",
        "shadow": "{??sys.shadow}",
        "icon": {
          "margin": "{??sys.icon.margin}",
          "learning": "0",
          "space": "{??sys.icon.space}"
        },
        "document": {
          "display": "none"
        },
        "frame": {
          "border": "{??sys.frame.border}",
          "borderOpacity": "{??sys.frame.borderOpacity}",
          "borderWidth": "{??sys.frame.borderWidth}"
        }
      },
      "mixin|m": {
        "appearance": {
          "contained": {
            "var|sys": {
              "text": "",
              "textOpacity": "",
              "backgroundOpacity": "",
              "gradient": "",
              "shadow": "",
              "selected": {
                "gradient": "{??sys.gradient}"
              },
              "disabled": {
                "text": "{??sys.text}",
                "textOpacity": "{??sys.textOpacity}",
                "backgroundOpacity": "{??sys.backgroundOpacity}",
                "shadow": "none"
              },
              "active": {
                "shadow": "{??sys.shadow}"
              }
            },
            "color": "{??l.text}",
            "gradient": "{??l.gradient}"
          },
          "elevated": {
            "var|sys": {
              "backgroundOpacity": "",
              "shadow": "",
              "disabled": {
                "text": "{??sys.text}",
                "textOpacity": "{??sys.textOpacity}",
                "backgroundOpacity": "{??sys.backgroundOpacity}",
                "shadow": "none"
              },
              "active": {
                "shadow": "{??sys.shadow}"
              }
            }
          },
          "tonal": {
            "var|sys": {
              "text": "",
              "backgroundOpacity": "",
              "frame": {
                "border": "{??sys.border}"
              },
              "disabled": {
                "text": "{??sys.text}",
                "textOpacity": "{??sys.textOpacity}",
                "backgroundOpacity": "{??sys.backgroundOpacity}"
              },
              "active": {
                "frame": {
                  "borderOpacity": "{??sys.borderOpacity}"
                }
              }
            },
            "&.??--readonly": {
              "color": "{??l.text}"
            },
            "::after": {
              "content": ""
            }
          },
          "outlined": {
            "var|sys": {
              "border": "{??sys.palette}",
              "borderOpacity": "",
              "frame": {
                "border": "{??sys.border}"
              },
              "disabled": {
                "textOpacity": ""
              },
              "active": {
                "frame": {
                  "borderOpacity": "{??sys.borderOpacity}"
                }
              }
            },
            "::before": {
              "content": ""
            },
            "::after": {
              "content": ""
            }
          },
          "text": {
            "var|sys": {
              "frame": {
                "border": "{??sys.border}"
              },
              "disabled": {
                "textOpacity": ""
              },
              "active": {
                "frame": {
                  "borderOpacity": "{??sys.borderOpacity}"
                }
              }
            },
            "::after": {
              "content": ""
            }
          }
        },
        "mixin|icon": {
          "var|l": {
            "padding": "{??sys.icon.padding}",
            "width": "var(--sys-height, auto)",
            "icon": {
              "learning": "0",
              "space": "0"
            }
          }
        },
        "mixin|text": {
          "var|l": {
            "padding": "{??sys.padding}",
            "width": "{??sys.width}",
            "icon": {
              "learning": "{??l.icon.margin} - {??l.padding}",
              "space": "{??sys.space}"
            },
            "document": {
              "display": "block"
            }
          }
        },
        "mixin|rounded": {
          "mixin|full": {
            "var|sys": {
              "icon": {
                "margin": "{??sys.icon.padding}",
                "fontSize": "{?icon.size} - ({??sys.space} * 2)"
              }
            },
            "var|?icon-size": "var(--sys-height) - ({??sys.icon.padding} * 2)"
          }
        }
      },
      "@style": "{??m.icon}",
      "@@ellipsis": "{d.font.ellipsis}",
      "width": {
        "auto": "auto",
        "full": "100%",
        "rename": "var|??sys.width"
      },
      "height": {
        "auto": "auto",
        "rename": "var|??sys.height"
      },
      "#text": {},
      "&.is-icon": {
        "@style": "{??m.icon}"
      },
      "~adaptive": {
        "full": {
          "@style": "{??m.text}"
        },
        "icon": {},
        "default": "full"
      },
      "~rounded": {
        "rename": "var|??sys.rounded"
      },
      "~dense": {
        "var|l": {
          "padding": "{??sys.dense.padding}",
          "icon": {
            "margin": "{??sys.dense.icon.margin}"
          }
        }
      },
      "~align": {
        "@style": "{d.font.align}"
      },
      "~selected": {},
      "~readonly": {},
      "~progress": {},
      "~focus": {},
      "~dragged": {}
    }
  }
}
