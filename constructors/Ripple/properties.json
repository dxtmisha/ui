{
  "d": {
    "ripple": {
      "var|_background": "0, 0, 0",
      "var|_inverse": "255, 255, 255",
      "var|_opacity": ".1",
      "var|_x": "50%",
      "var|_y": "50%",
      "animate|start": {
        "0%": {
          "background-opacity": "0",
          "clip-path": "circle(000% at {??_x} {??_y})"
        },
        "50%": {
          "background-opacity": "{??_opacity}",
          "clip-path": "circle(075% at {??_x} {??_y})"
        },
        "100%": {
          "background-opacity": "{??_opacity}",
          "clip-path": "circle(150% at {??_x} {??_y})"
        }
      },
      "stepEnd": {
        "opacity": "0",
        "transition-delay": "{d.sys.transitionDuration.fade.enter}",
        "transition-duration": "{d.sys.transitionDuration.fade.through}",
        "transition-property": "opacity"
      },
      "border-radius": "inherit",
      "inset": "0",
      "position": "absolute",
      "overflow": "hidden",
      "z-index": "1",
      "inverse": {
        "var|_background": "{??_inverse}"
      },
      "#item": {
        "animation-duration": "{d.sys.transitionDuration.fade.through}",
        "animation-fill-mode": "forwards",
        "animation-name": "??--start",
        "animation-timing-function": "linear",
        "background-color": "{??_background}",
        "inset": "0",
        "position": "absolute",
        "=isStepEnd": {
          ":not(:last-child)": {
            "@step": "{??stepEnd}"
          }
        }
      },
      ":not(:active)": {
        "#item": {
          "=isStepEnd": {
            "@step": "{??stepEnd}"
          }
        }
      }
    }
  }
}
