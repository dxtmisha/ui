{
  "d": {
    "ripple": {
      "var|_background": "255, 255, 255",
      "var|_opacity": ".1",
      "var|_duration": ".1",
      "var|_x": "50%",
      "var|_y": "50%",
      "!animate": {
        "0%": {
          "background-opacity": "0",
          "clip-path": "circle(000% at {?.ripple._x} {?.ripple._y})"
        },
        "100%": {
          "background-opacity": "{?.ripple._opacity}",
          "clip-path": "circle(150% at {?.ripple._x} {?.ripple._y})"
        }
      },
      "border-radius": "inherit",
      "inset": "0",
      "position": "absolute",
      "overflow": "hidden",
      "z-index": "1",
      "#item": {
        "animation-duration": "{d.sys.transitionDuration.fade.through}",
        "animation-fill-mode": "forwards",
        "animation-name": "?-ripple--animate",
        "background-color": "{?.ripple._background}",
        "inset": "0",
        "position": "absolute",
        "=isStepEnd": {
          ":not(:last-child)": {
            "opacity": "0",
            "transition-delay": "{d.sys.transitionDuration.fade.enter}",
            "transition-duration": "{d.sys.transitionDuration.fade.through}",
            "transition-property": "opacity"
          }
        }
      },
      ":not(:active)": {
        "#item": {
          "=isStepEnd": {
            "opacity": "0",
            "transition-delay": "{d.sys.transitionDuration.fade.enter}",
            "transition-duration": "{d.sys.transitionDuration.fade.through}",
            "transition-property": "opacity"
          }
        }
      }
    }
  }
}
