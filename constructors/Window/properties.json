{
  "d": {
    "window": {
      "var|sys": {
        "background": "255,255,255",
        "height": "600px",
        "index": "9999",
        "padding": "40px 16px",
        "shadow": "none",
        "transformOrigin": "center",
        "width": "800px",
        "scrim": {
          "background": "0,0,0",
          "opacity": "0.32"
        },
        "close": {
          "display": "none",
          "transition": "none",
          "body": {
            "opacity": "0",
            "transform": "none",
            "transition": "none"
          },
          "scrim": {
            "opacity": "0"
          }
        },
        "preparation": {
          "display": "flex",
          "transition": "{??sys.close.transition}",
          "body": {
            "opacity": "{??sys.close.body.opacity}",
            "transform": "scale(.8)",
            "transition": "{??sys.close.body.transition}"
          },
          "scrim": {
            "opacity": "{??sys.close.scrim.opacity}"
          }
        },
        "open": {
          "display": "{??sys.preparation.display}",
          "transition": "background {d.sys.transitionDuration.fade.enter} linear",
          "body": {
            "opacity": "1",
            "transform": "none",
            "transition": "opacity {d.sys.transitionDuration.xs} linear, transform {d.sys.transitionDuration.fade.enter} {d.sys.transitionFunction.deceleration}"
          },
          "scrim": {
            "opacity": "{??sys.scrim.opacity}"
          }
        },
        "hide": {
          "display": "{??sys.open.display}",
          "transition": "background {d.sys.transitionDuration.fade.exit} linear",
          "body": {
            "opacity": "0",
            "transform": "{??sys.open.body.transform}",
            "transition": "opacity {d.sys.transitionDuration.fade.exit} linear"
          },
          "scrim": {
            "opacity": "0"
          }
        }
      },
      "var|l": {
        "display": "{??sys.close.display}",
        "transition": "{??sys.close.transition}",
        "body": {
          "opacity": "{??sys.close.body.opacity}",
          "transform": "{??sys.close.body.transform}",
          "transition": "{??sys.close.body.transition}"
        },
        "scrim": {
          "opacity": "{??sys.close.scrim.opacity}"
        }
      },
      "var|_rounded": "0px",
      "align-items": "center",
      "background-color": "{??sys.scrim.background}",
      "background-opacity": "{??l.scrim.opacity}",
      "display": "{??l.display}",
      "flex-flow": "column",
      "inset": "0",
      "justify-content": "center",
      "padding": "{??sys.padding}",
      "position": "fixed",
      "transition": "{??l.transition}",
      "will-change": "=background-color, left, top, contents",
      "z-index": "{??sys.index}",
      "#body": {
        "background": "{??sys.background}",
        "border-radius": "{??_rounded}",
        "box-shadow": "{??sys.shadow}",
        "display": "flex",
        "flex-flow": "column",
        "flex-shrink": "1",
        "max-height": "{??sys.height}",
        "max-width": "{??sys.width}",
        "opacity": "{??l.body.opacity}",
        "overflow-y": "auto",
        "transform": "{??l.body.transform}",
        "transform-origin": "{??sys.transformOrigin}",
        "transition": "{??l.body.transition}",
        "width": "100%",
        "will-change": "opacity, visibility, transform, contents"
      },
      "#context": {
        "flex-grow": "1",
        "flex-shrink": "1"
      },
      "&[data-status='preparation']": {
        "var|l": {
          "display": "{??sys.preparation.display}",
          "transition": "{??sys.preparation.transition}",
          "body": {
            "opacity": "{??sys.preparation.body.opacity}",
            "transform": "{??sys.preparation.body.transform}",
            "transition": "{??sys.preparation.body.transition}"
          },
          "scrim": {
            "opacity": "{??sys.preparation.scrim.opacity}"
          }
        }
      },
      "&[data-status='open']": {
        "var|l": {
          "display": "{??sys.open.display}",
          "transition": "{??sys.open.transition}",
          "body": {
            "opacity": "{??sys.open.body.opacity}",
            "transform": "{??sys.open.body.transform}",
            "transition": "{??sys.open.body.transition}"
          },
          "scrim": {
            "opacity": "{??sys.open.scrim.opacity}"
          }
        }
      },
      "&[data-status='hide']": {
        "var|l": {
          "display": "{??sys.hide.display}",
          "transition": "{??sys.hide.transition}",
          "body": {
            "opacity": "{??sys.hide.body.opacity}",
            "transform": "{??sys.hide.body.transform}",
            "transition": "{??sys.hide.body.transition}"
          },
          "scrim": {
            "opacity": "{??sys.hide.scrim.opacity}"
          }
        }
      }
    }
  }
}
