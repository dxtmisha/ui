{
  "d": {
    "window": {
      "var|sys": {
        "var|background": "255,255,255",
        "var|height": "600px",
        "var|index": "9999",
        "var|padding": "40px 16px",
        "var|shadow": "none",
        "var|transformOrigin": "center",
        "var|width": "800px",
        "var|scrim": {
          "var|background": "0,0,0",
          "var|opacity": "0.32"
        },
        "var|close": {
          "var|display": "none",
          "var|transition": "none",
          "var|body": {
            "var|opacity": "0",
            "var|transform": "none",
            "var|transition": "none"
          },
          "var|scrim": {
            "var|opacity": "0"
          }
        },
        "var|preparation": {
          "var|display": "flex",
          "var|transition": "{??sys.close.transition}",
          "var|body": {
            "var|opacity": "{??sys.close.body.opacity}",
            "var|transform": "scale(.8)",
            "var|transition": "{??sys.close.body.transition}"
          },
          "var|scrim": {
            "var|opacity": "{??sys.close.scrim.opacity}"
          }
        },
        "var|open": {
          "var|display": "{??sys.preparation.display}",
          "var|transition": "background {d.sys.transitionDuration.fade.enter} linear",
          "var|body": {
            "var|opacity": "1",
            "var|transform": "none",
            "var|transition": "opacity {d.sys.transitionDuration.xs} linear, transform {d.sys.transitionDuration.fade.enter} {d.sys.transitionFunction.deceleration}"
          },
          "var|scrim": {
            "var|opacity": "{??sys.scrim.opacity}"
          }
        },
        "var|hide": {
          "var|display": "{??sys.open.display}",
          "var|transition": "background {d.sys.transitionDuration.fade.exit} linear",
          "var|body": {
            "var|opacity": "0",
            "var|transform": "{??sys.open.body.transform}",
            "var|transition": "opacity {d.sys.transitionDuration.fade.exit} linear"
          },
          "var|scrim": {
            "var|opacity": "0"
          }
        }
      },
      "var|l": {
        "var|display": "{??sys.close.display}",
        "var|transition": "{??sys.close.transition}",
        "var|body": {
          "var|opacity": "{??sys.close.body.opacity}",
          "var|transform": "{??sys.close.body.transform}",
          "var|transition": "{??sys.close.body.transition}"
        },
        "var|scrim": {
          "var|opacity": "{??sys.close.scrim.opacity}"
        }
      },
      "var|_rounded": "0px",
      "align-items": "center",
      "background-color": "{??sys.scrim.background}",
      "background-opacity": "{??l.scrim.opacity}",
      "display": "{??l.display}",
      "inset": "0",
      "justify-content": "center",
      "padding": "{??sys.padding}",
      "position": "fixed",
      "transition": "{??l.transition}",
      "will-change": "=background-color, left, top, contents",
      "z-index": "{??sys.index}",
      "#body": {
        "background": "{??sys.background}",
        "border-radius": "{??_rounded}",
        "box-shadow": "{??sys.shadow}",
        "max-height": "{??sys.height}",
        "max-width": "{??sys.width}",
        "opacity": "{??l.body.opacity}",
        "overflow-y": "auto",
        "transform": "{??l.body.transform}",
        "transform-origin": "{??sys.transformOrigin}",
        "transition": "{??l.body.transition}",
        "width": "100%",
        "will-change": "opacity, visibility, transform, contents"
      },
      "&[data-status='preparation']": {
        "var|l": {
          "var|display": "{??sys.preparation.display}",
          "var|transition": "{??sys.preparation.transition}",
          "var|body": {
            "var|opacity": "{??sys.preparation.body.opacity}",
            "var|transform": "{??sys.preparation.body.transform}",
            "var|transition": "{??sys.preparation.body.transition}"
          },
          "var|scrim": {
            "var|opacity": "{??sys.preparation.scrim.opacity}"
          }
        }
      },
      "&[data-status='open']": {
        "var|l": {
          "var|display": "{??sys.open.display}",
          "var|transition": "{??sys.open.transition}",
          "var|body": {
            "var|opacity": "{??sys.open.body.opacity}",
            "var|transform": "{??sys.open.body.transform}",
            "var|transition": "{??sys.open.body.transition}"
          },
          "var|scrim": {
            "var|opacity": "{??sys.open.scrim.opacity}"
          }
        }
      },
      "&[data-status='hide']": {
        "var|l": {
          "var|display": "{??sys.hide.display}",
          "var|transition": "{??sys.hide.transition}",
          "var|body": {
            "var|opacity": "{??sys.hide.body.opacity}",
            "var|transform": "{??sys.hide.body.transform}",
            "var|transition": "{??sys.hide.body.transition}"
          },
          "var|scrim": {
            "var|opacity": "{??sys.hide.scrim.opacity}"
          }
        }
      }
    }
  }
}
