{
  "d": {
    "window": {
      "var|sys": {
        "menu": {
          "index": "",
          "width": "",
          "height": "100vh",
          "background": "",
          "shadow": ""
        },
        "modal": {
          "index": "",
          "width": "",
          "height": "auto",
          "background": "",
          "shadow": ""
        },
        "scrim": {
          "opacity": "",
          "background": ""
        }
      },
      "var|l": {
        "display": "none",
        "index": "{??sys.modal.index}",
        "width": "{??sys.modal.width}",
        "height": "{??sys.modal.height}",
        "background": "{??sys.modal.background}",
        "shadow": "{??sys.modal.shadow}",
        "transition": "none",
        "transform-origin": "center",
        "body": {
          "opacity": "0",
          "transform": "none",
          "transition": "none"
        },
        "scrim": {
          "opacity": "0"
        }
      },
      "mixin|m": {
        "mixin|menu": {
          "var|sys": {
            "scrim": {
              "opacity": "0"
            }
          },
          "var|l": {
            "index": "{??sys.menu.index}",
            "width": "{??sys.menu.width}",
            "height": "{??sys.menu.height}",
            "background": "{??sys.menu.background}",
            "shadow": "{??sys.menu.shadow}",
            "transform-origin": "{??_originX} {??_originY}"
          },
          "content": "'--MENU--'",
          "top": "{??_insetY}",
          "right": "auto",
          "bottom": "auto",
          "left": "{??_insetX}",
          "padding": "0",
          "& .??__body": {
            "max-width": "100vw",
            "width": "{??_width}"
          },
          "&[data-status='preparation'], &[data-status='open'], &[data-status='hide']": {
            "var|l": {
              "display": "block"
            }
          }
        }
      },
      "animate|persistent-active": {
        "0%": {
          "transform": "scale(1)"
        },
        "50%": {
          "transform": "scale(1.02)"
        },
        "100%": {
          "transform": "scale(1)"
        }
      },
      "var|_width": "{??l.width}",
      "var|_height": "{??l.height}",
      "var|_padding": "0px",
      "var|_rounded": "0px",
      "var|_insetX": "0px",
      "var|_insetY": "0px",
      "var|_originX": "center",
      "var|_originY": "center",
      "#body": {},
      "#context": {},
      "#control": {},
      "&[data-status='preparation']": {
        "var|l": {
          "display": "flex",
          "body": {
            "transform": "scale(.8)"
          }
        }
      },
      "&[data-status='open']": {
        "var|l": {
          "display": "flex",
          "transition": "background {d.sys.transitionDuration.fade.enter} linear",
          "body": {
            "opacity": "1",
            "transform": "none",
            "transition": "opacity {d.sys.transitionDuration.xs} linear, transform {d.sys.transitionDuration.fade.enter} {d.sys.transitionFunction.deceleration}"
          },
          "scrim": {
            "opacity": "{??sys.scrim.opacity}"
          }
        }
      },
      "&[data-status='hide']": {
        "var|l": {
          "display": "flex",
          "transition": "background {d.sys.transitionDuration.fade.exit} linear",
          "body": {
            "opacity": "0",
            "transform": "none",
            "transition": "opacity {d.sys.transitionDuration.fade.exit} linear"
          },
          "scrim": {
            "opacity": "0"
          }
        }
      },
      "~adaptive": {
        "~menu": {
          "@style": "{??m.menu}"
        },
        "~modal": {}
      },
      "~alignment": {
        "bottom": {},
        "center": {},
        "left": {},
        "right": {},
        "top": {}
      },
      "~animationOrigin": {
        "auto": {},
        "bottom": {},
        "center": {},
        "left": {},
        "right": {},
        "top": {}
      },
      "~fullscreen": {},
      "~height": {
        "auto": {
          "var|height": "auto"
        },
        "full": {
          "var|height": "100%"
        }
      },
      "~width": {
        "auto": {
          "var|width": "auto",
          "& .??__body": {
            "width": "auto"
          }
        },
        "full": {
          "var|width": "100%"
        }
      }
    }
  }
}
