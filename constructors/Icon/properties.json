{
  "d": {
    "icon": {
      "var|_background": "0, 0, 0",
      "var|_opacity": ".08",
      "var|_rounded": "4px",
      "var|_size": "24px",
      "@select": "{d.selectNone}",
      "border-radius": "{??_rounded}",
      "display": "inline-block",
      "min-width": "{??_size}",
      "overflow": "hidden",
      "position": "relative",
      "transition-duration": "{d.sys.transitionDuration.fade.enter}",
      "transition-property": "clip-path, margin, opacity, transform",
      "squared": "{??_size}",
      "animationType": {
        "type2": {
          "clip-path": "inset(0 0 0 0)",
          "??--hide": {
            "clip-path": "inset(0 100% 0 0)",
            "margin": "0 calc({??_size} * -1) 0 0 !important",
            "opacity": "1",
            "transform": "none"
          }
        }
      },
      "section|background": {
        "background-color": "{??_background}",
        "background-opacity": "{??_opacity}"
      },
      "hide": {
        "@style": "{d.animation.hide}"
      },
      "size": {
        "sm": {
          "var|_size": "24px"
        },
        "md": {
          "var|_size": "40px"
        },
        "lg": {
          "var|_size": "56px"
        },
        "xl": {
          "var|_size": "100px",
          "width-aspect-ratio": "25/14"
        }
      },
      "turn": {
        "transform": "rotate(180deg)"
      }
    }
  }
}
