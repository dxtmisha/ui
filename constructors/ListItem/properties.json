{
  "d": {
    "list-item": {
      "var|sys": {
        "palette": "",
        "paddingX": "0px",
        "paddingY": "0px",
        "marginX": "0px",
        "marginY": "0px",
        "height": "",
        "text": "",
        "textOpacity": "",
        "background": "{??sys.palette}",
        "backgroundOpacity": "0",
        "gradient": "{??sys.palette}",
        "gradientOpacity": "0",
        "icon": {
          "margin": ""
        },
        "trailing": {
          "margin": ""
        },
        "prefix": {
          "text": "{??sys.description.text}",
          "textOpacity": "{??sys.description.textOpacity}",
          "padding": ""
        },
        "suffix": {
          "text": "{??sys.description.text}",
          "textOpacity": "{??sys.description.textOpacity}",
          "padding": ""
        },
        "description": {
          "text": "{??sys.text}",
          "textOpacity": "{??sys.textOpacity}",
          "clamp": "2",
          "padding": "0"
        },
        "hover": {
          "gradientOpacity": ""
        },
        "focus": {
          "gradientOpacity": ""
        },
        "active": {
          "gradientOpacity": ""
        },
        "highlight": {
          "background": "{??sys.highlight.palette}",
          "gradient": "{??sys.highlight.palette}"
        },
        "rounded": {
          "marginX": "0px",
          "marginY": "0px"
        },
        "selected": {
          "@style": "{??m.status}",
          "gradient": "{??sys.selected.palette}",
          "text": "{??sys.selected.palette}",
          "textOpacity": "",
          "description": {
            "text": "{??sys.selected.palette}"
          }
        },
        "disabled": {
          "@style": "{??m.status}",
          "background": "{??sys.disabled.palette}",
          "border": "{??sys.disabled.palette}",
          "gradient": "{??sys.disabled.palette}",
          "text": "{??sys.disabled.palette}",
          "textOpacity": "",
          "description": {
            "text": "{??sys.disabled.palette}"
          }
        }
      },
      "var|l": {
        "marginX": "{??sys.marginX}",
        "marginY": "{??sys.marginY}",
        "paddingX": "{??sys.paddingX}",
        "paddingY": "{??sys.paddingY}",
        "height": "{??sys.height}",
        "text": "{??sys.text}",
        "textOpacity": "{??sys.textOpacity}",
        "background": "{??sys.background}",
        "backgroundOpacity": "{??sys.backgroundOpacity}",
        "gradient": "{??sys.gradient}",
        "gradientOpacity": "{??sys.gradientOpacity}",
        "body": {
          "display": "flex"
        },
        "prefix": {
          "text": "{??sys.prefix.text}"
        },
        "suffix": {
          "text": "{??sys.suffix.text}"
        },
        "description": {
          "text": "{??sys.description.text}"
        }
      },
      "mixin|m": {
        "mixin|status": {
          "var|palette": "{??sys.palette}",
          "var|background": "{??sys.background}",
          "var|backgroundOpacity": "{??sys.backgroundOpacity}",
          "var|gradient": "{??sys.gradient}",
          "var|gradientOpacity": "{??sys.gradientOpacity}",
          "var|text": "{??sys.text}",
          "var|textOpacity": "{??sys.textOpacity}",
          "var|description": {
            "text": "{??sys.description.text}",
            "textOpacity": "{??sys.description.textOpacity}"
          },
          "var|prefix": {
            "text": "{??sys.prefix.text}",
            "textOpacity": "{??sys.prefix.textOpacity}"
          },
          "var|suffix": {
            "text": "{??sys.suffix.text}",
            "textOpacity": "{??sys.suffix.textOpacity}"
          }
        },
        "mixin|rounded": {
          "var|l": {
            "marginX": "{??sys.rounded.marginX}",
            "marginY": "{??sys.rounded.marginY}"
          }
        }
      },
      "var|_overlay": "{??l.background}",
      "#text": {},
      "#title": {},
      "#context": {},
      "#prefix": {},
      "#suffix": {},
      "#description": {},
      "section|overlay": {
        "var|l": {
          "backgroundOpacity": "{??sys.selected.backgroundOpacity} !important"
        }
      },
      "section|highlight": {
        "var|l": {
          "palette": "{??sys.highlight.palette} !important",
          "background": "{??sys.highlight.background} !important",
          "backgroundOpacity": "{??sys.highlight.backgroundOpacity} !important",
          "gradient": "{??sys.highlight.gradient} !important",
          "gradientOpacity": "{??sys.highlight.gradientOpacity}",
          "text": "{??sys.highlight.text} !important",
          "textOpacity": "{??sys.highlight.textOpacity} !important"
        }
      },
      "section|selected": {
        "var|l": {
          "palette": "{??sys.selected.palette} !important",
          "background": "{??sys.selected.background} !important",
          "backgroundOpacity": "{??sys.selected.backgroundOpacity} !important",
          "gradient": "{??sys.selected.gradient} !important",
          "gradientOpacity": "{??sys.selected.gradientOpacity}",
          "text": "{??sys.selected.text} !important",
          "textOpacity": "{??sys.selected.textOpacity} !important"
        }
      },
      "section|disabled": {
        "var|l": {
          "palette": "{??sys.disabled.palette} !important",
          "background": "{??sys.disabled.background} !important",
          "backgroundOpacity": "{??sys.disabled.backgroundOpacity} !important",
          "gradient": "{??sys.disabled.gradient} !important",
          "gradientOpacity": "{??sys.disabled.gradientOpacity} !important",
          "text": "{??sys.disabled.text} !important",
          "textOpacity": "{??sys.disabled.textOpacity} !important"
        }
      }
    }
  }
}
