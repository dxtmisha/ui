{
  "d": {
    "list-item": {
      "var|sys": {
        "palette": "",
        "paddingX": "0px",
        "paddingY": "0px",
        "marginX": "0px",
        "marginY": "0px",
        "height": "",
        "text": "",
        "textOpacity": "",
        "background": "{??l.palette}",
        "backgroundOpacity": "0",
        "gradient": "{??l.text}",
        "gradientOpacity": "0",
        "border": "",
        "borderOpacity": "",
        "borderWidth": "",
        "icon": {
          "margin": ""
        },
        "thumbnail": {
          "marginLeft": "0px",
          "marginRight": "{??sys.icon.margin}",
          "size": ""
        },
        "trailing": {
          "margin": ""
        },
        "prefix": {
          "text": "{??l.text}",
          "textOpacity": "{??l.description.textOpacity}",
          "padding": ""
        },
        "suffix": {
          "text": "{??l.text}",
          "textOpacity": "{??l.description.textOpacity}",
          "padding": ""
        },
        "description": {
          "text": "{??l.text}",
          "textOpacity": "{??l.textOpacity}",
          "clamp": "2",
          "padding": "0"
        },
        "overlay": {
          "backgroundOpacity": ""
        },
        "highlight": {
          "gradient": "{??l.palette}"
        },
        "selected": {
          "text": "{??l.palette}"
        },
        "progress": {
          "size": "",
          "opacity": ""
        },
        "disabled": {
          "text": "{??l.palette}",
          "textOpacity": "",
          "description": {
            "textOpacity": "{??l.textOpacity}"
          }
        },
        "hover": {
          "gradientOpacity": ""
        },
        "focus": {
          "gradientOpacity": ""
        },
        "active": {
          "gradientOpacity": ""
        },
        "rounded": {
          "size": "none",
          "marginX": "0px",
          "marginY": "0px"
        }
      },
      "var|l": {
        "palette": "{??sys.palette}",
        "marginX": "{??sys.marginX}",
        "marginY": "{??sys.marginY}",
        "paddingX": "{??sys.paddingX}",
        "paddingY": "{??sys.paddingY}",
        "height": "{??sys.height}",
        "text": "{??sys.text}",
        "textOpacity": "{??sys.textOpacity}",
        "background": "{??sys.background}",
        "backgroundOpacity": "{??sys.backgroundOpacity}",
        "gradient": "{??sys.gradient}",
        "gradientOpacity": "{??sys.gradientOpacity}",
        "rounded": "{??sys.rounded.size}",
        "body": {
          "display": "flex"
        },
        "short": {
          "display": "none"
        },
        "prefix": {
          "text": "{??sys.prefix.text}",
          "textOpacity": "{??sys.prefix.textOpacity}"
        },
        "suffix": {
          "text": "{??sys.suffix.text}",
          "textOpacity": "{??sys.suffix.textOpacity}"
        },
        "description": {
          "text": "{??sys.description.text}",
          "textOpacity": "{??sys.description.textOpacity}"
        }
      },
      "mixin|m": {
        "mixin|appearance": {
          "contained": {
            "var|sys": {
              "selected": {
                "text": "",
                "backgroundOpacity": ""
              }
            }
          }
        },
        "mixin|rounded": {
          "var|l": {
            "marginX": "{??sys.rounded.marginX}",
            "marginY": "{??sys.rounded.marginY}"
          }
        },
        "mixin|icon": {
          "var|sys": {
            "icon": {
              "margin": "0px"
            },
            "thumbnail": {
              "marginRight": "{??sys.thumbnail.marginLeft}"
            }
          },
          "var|l": {
            "body": {
              "display": "none"
            },
            "short": {
              "display": "block"
            }
          },
          "flex-wrap": "wrap",
          "align-content": "center",
          "justify-content": "center",
          "padding": "{??sys.paddingY}"
        }
      },
      "var|_overlay": "{??l.background}",
      "height": {
        "rename": "var|l.height"
      },
      "#body": {},
      "#text": {},
      "#title": {},
      "#context": {},
      "#prefix": {},
      "#suffix": {},
      "#description": {},
      "#short": {},
      "~overlay": {
        "var|l": {
          "backgroundOpacity": "{??sys.overlay.backgroundOpacity} !important"
        }
      },
      "~appearance": {},
      "~adaptive": {},
      "~rounded": {
        "none": {
          "var|l": {
            "rounded": "none"
          }
        },
        "rename": "var|l.rounded"
      },
      "~highlight": {},
      "~border": {},
      "~selected": {},
      "~readonly": {},
      "~progress": {},
      "~disabled": {},
      "&.is-icon": {
        "@style": "{??m.icon}"
      }
    }
  }
}
