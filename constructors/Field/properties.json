{
  "d": {
    "field": {
      "mixin|m": {
        "mixin|status": {
          "var|palette": "{??sys.palette}",
          "var|background": "{??sys.background}",
          "var|backgroundOpacity": "{??sys.backgroundOpacity}",
          "var|border": "{??sys.border}",
          "var|borderOpacity": "{??sys.borderOpacity}",
          "var|borderWidth": "{??sys.borderWidth}",
          "var|gradient": "{??sys.gradient}",
          "var|gradientOpacity": "{??sys.gradientOpacity}",
          "var|shadow": "{??sys.shadow}",
          "var|shift": "{??sys.shift}",
          "var|text": "{??sys.text}",
          "var|textOpacity": "{??sys.textOpacity}",
          "var|label": {
            "var|opacity": "{??sys.label.opacity}",
            "var|scale": "{??sys.label.scale}",
            "var|shift": "{??sys.label.shift}",
            "var|text": "{??sys.label.text}",
            "var|textOpacity": "{??sys.label.textOpacity}"
          },
          "var|prefix": {
            "var|opacity": "{??sys.prefix.opacity}",
            "var|text": "{??sys.prefix.text}",
            "var|textOpacity": "{??sys.prefix.textOpacity}"
          }
        },
        "mixin|borderBottom": {
          "border-radius": "0",
          "bottom": "0",
          "left": "0",
          "right": "0",
          "position": "absolute"
        },
        "mixin|appearance": {
          "mixin|filled": {
            "var|sys": {
              "backgroundOpacity": "",
              "borderOpacity": "",
              "disabled": {
                "borderOpacity": ""
              },
              "focus": {
                "border": "",
                "borderOpacity": "",
                "shift": "{??sys.label.height} / 3",
                "label": {
                  "scale": "0.875",
                  "shift": "{??sys.label.height} / 2"
                }
              }
            },
            "var|l": {
              "borderWidth": "0"
            },
            "& .??__body": {
              "border-bottom-left-radius": "0",
              "border-bottom-right-radius": "0"
            },
            "& .??__prefix, & .??__suffix": {
              "var|l": {
                "shift": "{??sys.focus.shift}"
              }
            },
            "& .??__input": {},
            "& .??__border": {
              "after": {
                "@style": "{??m.borderBottom}",
                "palette-background": "{??sys.focus.border}",
                "background-opacity": "{??sys.focus.borderOpacity}",
                "clip-path": "inset(0 50%)",
                "height": "{??sys.focus.borderWidth}",
                "transition-duration": "{??sys.transitionDuration}",
                "transition-property": "clip-path",
                "transition-function": "{d.sys.transitionFunction.standard}",
                "z-index": "8"
              },
              "before": {
                "@style": "{??m.borderBottom}",
                "background-color": "{??l.border}",
                "background-opacity": "{??l.borderOpacity}",
                "height": "{??sys.borderWidth}"
              }
            },
            "&:not(.??--disabled, .??--readonly)": {
              "& .??__body": {
                "hover": {
                  "var|l": {
                    "borderOpacity": "{??sys.hover.borderOpacity}",
                    "gradientOpacity": "{??sys.hover.gradientOpacity}"
                  }
                }
              },
              "&:focus-within, &.??--focus": {
                "& .??__body": {
                  "var|l": {
                    "gradientOpacity": "{??sys.focus.gradientOpacity}"
                  }
                },
                "& .??__border::after": {
                  "clip-path": "inset(0 0)"
                }
              }
            },
            "&:focus-within, &.??--focus, &.is-value": {
              "var|l": {
                "gradientOpacity": "{??sys.focus.gradientOpacity}",
                "shift": "{??sys.focus.shift}",
                "label": {
                  "scale": "{??sys.focus.label.scale}",
                  "shift": "{??sys.focus.label.shift}"
                },
                "prefix": {
                  "opacity": "{??sys.focus.prefix.opacity}"
                }
              }
            },
            "&.??--disabled": {
              "var|l": {
                "borderOpacity": "{??sys.disabled.borderOpacity}"
              }
            }
          },
          "mixin|outlined": {}
        }
      },
      "var|sys": {
        "palette": "",
        "background": "",
        "backgroundOpacity": "",
        "border": "",
        "borderOpacity": "",
        "borderWidth": "",
        "gradient": "",
        "gradientOpacity": "",
        "height": "",
        "padding": "",
        "shadow": "",
        "shift": "0px",
        "space": "",
        "text": "",
        "textOpacity": "",
        "transitionDuration": "{d.sys.transitionDuration.fade.enter}",
        "width": "",
        "label": {
          "height": "",
          "opacity": "1",
          "required": "",
          "scale": "1",
          "shift": "0px",
          "text": "{??sys.text}",
          "textOpacity": "{??sys.textOpacity}"
        },
        "prefix": {
          "opacity": "0",
          "text": "{??sys.text}",
          "textOpacity": "{??sys.textOpacity}"
        },
        "active": {
          "@style": "{??m.status}"
        },
        "disabled": {
          "@style": "{??m.status}"
        },
        "focus": {
          "@style": "{??m.status}",
          "prefix": {
            "opacity": "1"
          }
        },
        "hover": {
          "@style": "{??m.status}"
        },
        "selected": {
          "@style": "{??m.status}",
          "var|background": "{??sys.palette}",
          "var|gradient": "{??sys.palette}",
          "var|text": "{??sys.palette}"
        }
      },
      "var|l": {
        "palette": "{??sys.palette}",
        "background": "{??sys.background}",
        "backgroundOpacity": "{??sys.backgroundOpacity}",
        "border": "{??sys.border}",
        "borderOpacity": "{??sys.borderOpacity}",
        "borderWidth": "{??sys.borderWidth}",
        "gradient": "{??sys.gradient}",
        "gradientOpacity": "{??sys.gradientOpacity}",
        "shadow": "{??sys.shadow}",
        "shift": "{??sys.shift}",
        "text": "{??sys.text}",
        "textOpacity": "{??sys.textOpacity}",
        "label": {
          "opacity": "{??sys.label.opacity}",
          "scale": "{??sys.label.scale}",
          "shift": "{??sys.label.shift}",
          "text": "{??sys.label.text}",
          "textOpacity": "{??sys.label.textOpacity}",
          "translate": {
            "x": "{??_valueLeft}",
            "y": "{??sys.height} / 4"
          }
        },
        "prefix": {
          "opacity": "{??sys.prefix.opacity}",
          "text": "{??sys.prefix.text}",
          "textOpacity": "{??sys.prefix.textOpacity}"
        }
      },
      "?ripple-background": "{??l.background}",
      "height": {
        "rename": "var|sys.height"
      },
      "width": {
        "auto": "auto",
        "full": "100%",
        "rename": "var|sys.width"
      },
      "section|align": {
        "center": {},
        "left": {},
        "right": {}
      },
      "section|cancel": {
        "auto": {},
        "always": {}
      },
      "section|disabled": {},
      "section|focus": {},
      "section|readonly": {},
      "section|selected": {},
      "#body": {},
      "#border": {},
      "#hidden": {},
      "#input": {},
      "#label": {},
      "#prefix": {},
      "#required": {},
      "#scoreboard": {
        "#context": {},
        "#space": {}
      },
      "#suffix": {},
      "#text": {},
      "#title": {}
    }
  }
}
